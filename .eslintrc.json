{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "linebreak-style": "off",
    "semi": "off",
    "no-console": "off",
    "import/extensions": "off",
    "arrow-body-style": "off",
    "no-return-await": "off",
    "implicit-arrow-linebreak": "off",
    "import/prefer-default-export": "off",
    "consistent-return": "off",
    "object-curly-newline": "off"
  }
}
